{
  "name": "typescript-node-template",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "dev": "npm run clean && nodemon",
    "compile": "cross-env NODE_ENV=development minify=false node esbuild.mjs",
    "typecheck": "tsc --noEmit",
    "build": "npm run clean && npm run typecheck && cross-env minify=true node esbuild.mjs",
    "tests": "vitest --watch false --config ./vitest.config.ts",
    "tests:watch": "npm run tests -- --watch",
    "tests:coverage": "npm run tests -- --coverage",
    "tests:ui": "npm run tests -- --ui --watch",
    "lint": "eslint ./src/**/*",
    "lint:fix": "npm run lint -- --fix",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "saiumesh535 (https://saiumesh.dev/)",
  "license": "MIT",
  "volta": {
    "node": "19.5.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@types/source-map-support": "^0.5.6",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "@vitest/coverage-c8": "^0.28.1",
    "cross-env": "^7.0.3",
    "esbuild": "^0.17.4",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^9.0.0",
    "husky": "^8.0.3",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.3",
    "rimraf": "^4.1.2",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "tsc-watch": "^6.0.0",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.4",
    "vite-tsconfig-paths": "^4.0.5",
    "vitest": "^0.28.1"
  }
}
